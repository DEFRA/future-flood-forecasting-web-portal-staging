<?xml version="1.0" encoding="UTF-8"?> 
<databaseChangeLog 
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd"> 
  <changeSet author="pwadmore" id="changelog-copy-data-to-datetimeoffset-timeseries-table">
    <preConditions onFail="HALT">
      <sqlCheck expectedResult="0">SELECT COUNT(*) FROM ${fffsWebPortalStagingDbStagingSchema}.TIMESERIES</sqlCheck>
    </preConditions>
	  <sql>
      INSERT INTO ${fffsWebPortalStagingDbStagingSchema}.TIMESERIES
        SELECT
          *
        FROM
          ${fffsWebPortalStagingDbStagingSchema}.DATETIME2_TIMESERIES
    </sql>
	  <rollback>
		  <sql>
        DELETE FROM ${fffsWebPortalStagingDbStagingSchema}.TIMESERIES
      </sql>
	  </rollback>
  </changeSet>
</databaseChangeLog> 
